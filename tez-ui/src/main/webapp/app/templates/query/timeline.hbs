{{#if this.loaded}}

  {{#if this.model.perf}}
    <QueryTimeline @perf={{this.model.perf}} />

    <EmTable @columns={{this.columns}} @rows={{this.rows}} @rowCount={{this.rows.length}} @definition={{this.definition}} @enablePagination={{false}} @searchAction="searchChanged" @sortAction="sortChanged" />
  {{else}}
    <h2>Data not available to display Timeline!</h2>
    <h4>You may be using an older version of Hive.</h4>
  {{/if}}

{{else}}
  <Loading/>
{{/if}}
