{{#if this.loaded}}
  <EmTable
    @columns={{this.visibleColumns}}
    @rows={{this.model}}
    @rowCount={{this.rowCount}}

    @classNames="all-dags-table"

    @headerComponentNames={{this.headerComponentNames}}
    @footerComponentNames={{this.footerComponentNames}}

    @definition={{this.definition}}
    @enableSort={{false}}

    @rowCountChanged={{this.rowCountChanged}}

    @loadPage="loadPage"
    @rowsChanged={{this.tableRowsChanged}}
    @openColumnSelector={{this.openColumnSelector}}
    @homeSearch={{this.homeSearch}}
  />
  {{#if this.queue}}
    <div class="alert alert-info">
      <strong>Info!</strong> Searching on Queue Name is supported only from <strong>Tez 0.9.0</strong>. So DAGs created by older versions of Tez will not be available!
    </div>
  {{/if}}
{{else}}
  <Loading/>
{{/if}}
