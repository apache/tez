{{#if this.normalizedLinks.length}}
  {{#each this.normalizedLinks as |link|}}
    {{#if link.routeName}}
      {{#if link.withModel}}
        <LinkTo @route={{link.routeName}} @model={{link.model}} target={{this.definition.target}}>
          {{link.text}}
        </LinkTo>
      {{else}}
        <LinkTo @route={{link.routeName}} target={{this.definition.target}}>
          {{link.text}}
        </LinkTo>
      {{/if}}
    {{else if link.href}}
      <a href={{link.href}} target={{this.definition.target}} download={{link.download}}>
        {{link.text}}
      </a>
    {{else}}
      {{link.text}}
    {{/if}}
  {{/each}}
{{else}}
  <span class="txt-message">Not Available!</span>
{{/if}}
